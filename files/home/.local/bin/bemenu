#!/usr/bin/env sh

# Wrapper for bemenu that adds some default options.

exec /usr/bin/bemenu -i -b -n \
    --fn 'monospace 12' \
    --tb='#222222' --tf='#ffffff' \
    --fb='#222222' --ff='#bbbbbb' \
    --nb='#222222' --nf='#bbbbbb' \
    --hb='#005577' --hf='#eeeeee' \
    --sb='#667f33' --sf='#eeeeee' \
    "$@"
